{
  "name": "payload-test",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "bun lint",
    "build": "bun next build",
    "dev": "bun next dev --turbo",
    "devsafe": "rm -rf .next && bun next dev",
    "generate:types": "bun payload generate:types",
    "generate:schema": "bun payload-graphql generate:schema",
    "db:seed:run": "bun tsx ./src/database/seed",
    "db:migrate:run": "bun payload migrate",
    "db:migrate:create": "bun payload migrate:create",
    "lint": "bun eslint .",
    "format": "prettier .",
    "start": "bun next start",
    "test": "bun test",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.0.0-beta.100",
    "@payloadcms/db-sqlite": "3.0.0-beta.100",
    "@payloadcms/email-nodemailer": "3.0.0-beta.100",
    "@payloadcms/next": "3.0.0-beta.100",
    "@payloadcms/richtext-lexical": "3.0.0-beta.100",
    "babel-plugin-react-compiler": "0.0.0-experimental-7449567-20240905",
    "graphql": "16.9.0",
    "next": "15.0.0-canary.146",
    "payload": "3.0.0-beta.100",
    "react": "19.0.0-rc-a03254bc-20240905",
    "react-dom": "19.0.0-rc-a03254bc-20240905",
    "tsx": "^4.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@payloadcms/graphql": "3.0.0-beta.100",
    "@types/bun": "latest",
    "@types/node": "^22.5.4",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.10.0",
    "eslint-config-next": "15.0.0-canary.146",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.6.0"
  }
}
